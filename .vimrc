set number
set title
set cursorline
set autoindent
set smartindent
set encoding=utf-8
set showcmd
syntax enable

set tabstop=4
set expandtab
set shiftwidth=4
set backspace=indent,eol,start

set clipboard=unnamed,autoselect

set hlsearch

set history=10000

inoremap jk <ESC>

" AutoSaveVim
let g:asv_delay = 500
